{
    "$schema": "redfish-schema.v1_2_0.json",
    "$ref": "Zone.v1_0_2.json#/definitions/Zone",
    "copyright": "Copyright 2014-2017 Distributed Management Task Force, Inc. (DMTF). For the full DMTF copyright policy, see http://www.dmtf.org/about/policies/copyright",
    "definitions": {
        "Zone": {
            "additionalProperties": false,
            "properties": {
                "Description": {
                    "readonly": true,
                    "anyOf": [
                        {
                            "$ref": "Resource.json#/definitions/Description"
                        },
                        {
                            "type": "null"
                        }
                    ]
                },
                "Links": {
                    "additionalProperties": false,
                    "properties": {
                        "InvolvedSwitches@odata.count": {
                            "$ref": "odata.4.0.0.json#/definitions/count"
                        },
                        "Endpoints@odata.count": {
                            "$ref": "odata.4.0.0.json#/definitions/count"
                        },
                        "InvolvedSwitches@odata.navigationLink": {
                            "type": "string",
                            "format": "uri"
                        },
                        "Oem": {
                            "$ref": "Resource.json#/definitions/Oem",
                            "longDescription": "This object represents the Oem property.  All values for resources described by this schema shall comply to the requirements as described in the Redfish specification.",
                            "description": "Oem extension object."
                        },
                        "Endpoints@odata.navigationLink": {
                            "type": "string",
                            "format": "uri"
                        },
                        "InvolvedSwitches": {
                            "type": "array",
                            "readonly": true,
                            "longDescription": "The value of this property shall be a reference to the resources that this zone is associated with and shall reference a resource of type Switch.",
                            "items": {
                                "$ref": "Switch.json#/definitions/Switch"
                            },
                            "description": "An array of references to the switchs that are utilized in this zone."
                        },
                        "Endpoints": {
                            "type": "array",
                            "readonly": true,
                            "longDescription": "The value of this property shall be a reference to the resources that this zone is associated with and shall reference a resource of type Endpoint.",
                            "items": {
                                "$ref": "Endpoint.json#/definitions/Endpoint"
                            },
                            "description": "An array of references to the endpoints that are contained in this zone."
                        }
                    },
                    "patternProperties": {
                        "^([a-zA-Z_][a-zA-Z0-9_]*)?@(odata|Redfish|Message|Privileges)\\.[a-zA-Z_][a-zA-Z0-9_.]+$": {
                            "type": [
                                "array",
                                "boolean",
                                "number",
                                "null",
                                "object",
                                "string"
                            ],
                            "description": "This property shall specify a valid odata or Redfish property."
                        }
                    },
                    "type": "object",
                    "longDescription": "The Links property, as described by the Redfish Specification, shall contain references to resources that are related to, but not contained by (subordinate to), this resource.",
                    "description": "Contains references to other resources that are related to this resource."
                },
                "@odata.context": {
                    "$ref": "odata.4.0.0.json#/definitions/context"
                },
                "Name": {
                    "readonly": true,
                    "$ref": "Resource.json#/definitions/Name"
                },
                "Status": {
                    "$ref": "Resource.json#/definitions/Status"
                },
                "Id": {
                    "readonly": true,
                    "$ref": "Resource.json#/definitions/Id"
                },
                "@odata.type": {
                    "$ref": "odata.4.0.0.json#/definitions/type"
                },
                "@odata.id": {
                    "$ref": "odata.4.0.0.json#/definitions/id"
                },
                "Oem": {
                    "$ref": "Resource.json#/definitions/Oem",
                    "longDescription": "The value of this string shall be of the format for the reserved word *Oem*.",
                    "description": "This is the manufacturer/provider specific extension moniker used to divide the Oem object into sections."
                }
            },
            "patternProperties": {
                "^([a-zA-Z_][a-zA-Z0-9_]*)?@(odata|Redfish|Message|Privileges)\\.[a-zA-Z_][a-zA-Z0-9_.]+$": {
                    "type": [
                        "array",
                        "boolean",
                        "number",
                        "null",
                        "object",
                        "string"
                    ],
                    "description": "This property shall specify a valid odata or Redfish property."
                }
            },
            "type": "object",
            "required": [
                "Id",
                "Name"
            ],
            "longDescription": "This resource shall be used to represent a simple zone for a Redfish implementation.",
            "description": "Switch contains properties describing a simple fabric zone."
        }
    },
    "title": "#Zone.v1_0_2.Zone"
}